# microservice specific routes

GET        /user-answers                                        controllers.CacheController.getAll(lrn: Option[String] ?= None, limit: Option[Int] ?= None, skip: Option[Int] ?= None, sortBy: Option[Sort] ?= None)
GET        /user-answers/:lrn                                   controllers.CacheController.get(lrn: String)
POST       /user-answers                                        controllers.CacheController.post()
PUT        /user-answers                                        controllers.CacheController.put()
DELETE     /user-answers/:lrn                                   controllers.CacheController.delete(lrn: String)

